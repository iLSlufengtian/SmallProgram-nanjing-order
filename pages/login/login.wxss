.modal-mask {
  width: 100%;
  height: 100%;
  position: fixed;
  top: 0;
  left: 0;
  background: #000;
  opacity: 0.5;
  overflow: hidden;
  z-index: 9000;
  color: #fff;
  border-radius: 20rpx;
}

.modal-photo {
  position: fixed;
  top: 35%;
  left: 0;
  z-index: 9999;
  margin: -40rpx 270rpx;
}

.start{
  width: 204rpx;
  height:156rpx;
}

.modal-content {
  position: fixed;
  top: 45%;
  left: 0;
  z-index: 9500;
  width: 590rpx;
  height: 340rpx;
  overflow: hidden;
  /* padding: 120rpx 50rpx 50rpx; */
  background: #fff;
  margin: -110rpx 75rpx;
